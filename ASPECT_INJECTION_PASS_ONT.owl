    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasDataAccessRight -->

    <owl:ObjectProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasDataAccessRight">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#hasRelationToModelComponent"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:comment xml:lang="en">This object property links together pointcuts and data access rights.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
    </owl:ObjectProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointDescription -->

    <owl:ObjectProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointDescription">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#hasRelationToModelComponent"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:comment xml:lang="en">This object property links together pointcuts and join point descriptions.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasDigitalSignature -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasDigitalSignature">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">For now the digital signature was defined to be of the type xsd:string. This may need to be changed in future versions of the extended ontology to better account for existing formats dedicated to representing digital signatures.</rdfs:comment>
        <rdfs:comment xml:lang="en">This data property assigns a textual digital signature representation to a pointcut.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasExecutionModality -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasExecutionModality">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:executionModality"/>
        <rdfs:comment xml:lang="en">This data property assigns an execution modality (using a dedicated datatype) to a join point description.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:executionModality"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasInjectionPriority -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasInjectionPriority">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#positiveInteger"/>
        <rdfs:comment xml:lang="en">This data property assigns an injection priority to a pointcut. The priority is a positive integer and is used to govern the execution order of multiple advice models that apply to the same join point in a core process model.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#Pointcut"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointQuery -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointQuery">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">This data property assigns a textual join point query to a join point description. The query is intended to take the form of a pattern definition that is used to match, e.g., state names of join point states (defined through the join point type).</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointType -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasJoinPointType">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:joinPointType"/>
        <rdfs:comment xml:lang="en">This data property assigns a join point type (using a dedicated datatype) to a join point description.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:joinPointType"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasOperationType -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasOperationType">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:operationType"/>
        <rdfs:comment xml:lang="en">This data property assigns an operation type (using a dedicated datatype) to a data access right.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:operationType"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasSubjectDataType -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasSubjectDataType">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:subjectDataType"/>
        <rdfs:comment xml:lang="en">This data property assigns a subject data type (using a dedicated datatype) to a data access right.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#DataAccessRight"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:subjectDataType"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.ce.jku.edu/aspect-injection-pass-ont#hasWeavingModality -->

    <owl:DatatypeProperty rdf:about="http://www.ce.jku.edu/aspect-injection-pass-ont#hasWeavingModality">
        <rdfs:subPropertyOf rdf:resource="http://www.i2pm.net/standard-pass-ont#PASSModelDataProperty"/>
        <rdfs:domain rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:range rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:weavingModality"/>
        <rdfs:comment xml:lang="en">This data property assigns a weaving modality (using a dedicated datatype) to a join point description.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#JoinPointDescription"/>
        <rdfs:seeAlso rdf:resource="http://www.ce.jku.edu/aspect-injection-pass-ont#aip:weavingModality"/>
    </owl:DatatypeProperty>
    


    <!-- 
